public class Main
{
	public static void main(String[] args) {
	    //HashSet in java
	    HashSet<Integer> set=new HashSet<>();   // creation
	    set.add(6); // adding value
	    set.contains(6);    // return true if present otherwise reutns false
	    set.remove(6);  // removing value
	    set.isEmpty();  // return true/false 
	    set.clear();        // clears the set
	    Iterator it=set.Iterator(); // iterating the set
	    while(it.hasNext()) 
	    {
	        System.out.print(it.next());
	    }
	    
	    //HashMap in java
	    HashMap<Integer,Integer> map=new HashMap<>();
	    map.put(1,3);   // adding value
	    if(map.containsKey(5)) map.get(5);     // checking if value present and getting value
	    map.remove(1);  // removing value 
	    for(Map.Entry<Integer,Integer> e : Map.entrySet())  // traversing the whole map
	    {
	        int key=e.getKey();
	        int value=e.getValue();
	    }
	}
}
